    </div>
  </div>

<div id="listing-show-page"> 
      <div class="listing-text-content">
        <p class="listing-title"><%= current_listing.title %></p>
        <p class="listing-additional-info"><%= listing_type[:subtype]%></p>
        <% if url_for(:only_path => true) == "/listings" %>
         <%= link_to "View more", listing_path(listing.id) %>
        <% end %>
        
        <% if signed_in? && (current_user.id == current_listing.user_id || current_user.superadmin?) %>
          <p><%= link_to "Edit this listing", edit_listing_path(listing.id) %></p>
        <% elsif signed_in? %>
          <p><%= link_to "Book this property!", book_path(current_listing.id) %></p>
        <% end %>
      </div>
  <%= link_to "View all", listings_path %>
</div>